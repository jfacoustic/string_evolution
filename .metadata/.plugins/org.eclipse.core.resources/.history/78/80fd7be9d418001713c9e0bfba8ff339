#ifndef POPULATION_H_
#define POPULATION_H_
#include <vector>
#include "Organism.h"
class Population {
public:
	Population(int count, std::string name);
	void addOrganism(std::string sequence);
	void killOrganism(int id);
	virtual ~Population();

private:
	int _count;
	std::string _name;
	std::vector <Organism> pop;
};

#endif /* POPULATION_H_ */
