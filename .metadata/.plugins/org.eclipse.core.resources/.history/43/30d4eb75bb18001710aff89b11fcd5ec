#ifndef ORGANISM_H_
#define ORGANISM_H_
#include <string>
class Organism {
public:
	Organism(std::string sequence);
	virtual ~Organism();
	Organism reproduce(double mutation_probability);
private:
	std::string _sequence;
};

#endif /* ORGANISM_H_ */
