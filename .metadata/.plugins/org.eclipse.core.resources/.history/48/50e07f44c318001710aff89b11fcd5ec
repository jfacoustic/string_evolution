#include <iostream>
#include <ctime>
#include <cstdlib>
#include "Organism.h"
using namespace std;

int main() {
	Organism * test = new Organism("AGTCA");
	srand(time(NULL));
	for (int i = 0; i < 100; i ++) {
		Organism * testChild = test->reproduce(40);
		cout << testChild->getSequence() << endl;
		delete testChild;
	}
	return 0;
}
